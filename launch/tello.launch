<?xml version="1.0"?>
<launch>
    <arg name="name" default="camera" />
    <group ns="$(arg name)">
        <node pkg="ros_tello" name="camera" type="ros_tello.py" output="screen"/>
    	<node name="republish" type="republish" pkg="image_transport" output="screen" args="raw in:=image_raw compressed out:=image_raw" />
    </group>
</launch>
